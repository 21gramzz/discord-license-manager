    "react-dom": "17.0.2",
    "react-hook-form": "^6.15.4",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/client-preset": "1.1.3",
